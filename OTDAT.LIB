(TOOL SET DATA)
OTDAT

(MACHINE: MU6300V-L - OSP P500)
(PROGRAMMER: JUSTIN BROWN)
(REV: 0)
(DATE: 07/29/2024)

(NOTES:)
  (ANY EDITS TO THIS PROGRAM REQUIRES LIBRARY PROGRAM RE-REGISTRATION)
    (AUTO MODE -> LIBRARY PROGRAM SOFTKEY)
    (FIND OMTM4 -> DELETE -- ONLY WIPES FROM REGISTER)
    (REGISTER .LIB -> FIND OMTM4 -> OK)

  (PASSED VARIABLES:)
    (PSRT = OVERWRITE VC# START)
    (PEND = OVERWRITE VC# STOP)

(ADDING TOOL:)
  (ADD "JUMP LINE" TO "TOOL LIBRARY" SECTION)
  (EXAMPLE:)
   (IF[VTLCN EQ 1000] N1000)
   (EXPLAINATION: IF TOOL IN SPINDLE = T1000, THEN JUMP TO LINE N1000)

  (ADD VARIABLES TO "NON-CENTER TOOL SET VARIABLES":)
    (EXAMPLE:)
      (N1000)
      (PX=12.5 PY=0 PRS=70 PINS=3)
      (GOTO NEND)

    (VARIABLE DEFINITIONS:)
      (PX = [TOOL RADIUS - 1.0mm])
      (PY = ALWAYS 0)
      (PRS = TOOL ROTATION ANGLE -- *MUST BE LOWEST NUMBER FOR MULTI-SET*)
      (PINS = # OF INSERTS -- MULTI-SET ONLY)

      

(-------------------------------START EDITS------------------------------)
(TOOL LIBRARY)
    (CENTER SET TOOLS)
        (SOLID DRILLS)
        IF[VTLCN EQ 100050] N1
        IF[VTLCN EQ 100060] N1
        IF[VTLCN EQ 100080] N1
        IF[VTLCN EQ 100090] N1
        IF[VTLCN EQ 100100] N1
        IF[VTLCN EQ 100110] N1
        IF[VTLCN EQ 100170] N1
        IF[VTLCN EQ 100180] N1
        IF[VTLCN EQ 100190] N1
        IF[VTLCN EQ 100200] N1
        IF[VTLCN EQ 100210] N1
        IF[VTLCN EQ 100220] N1

        (INSERT DRILLS - MUST BE POINTED)
        IF[VTLCN EQ 150010] N1
        IF[VTLCN EQ 150040] N1
        IF[VTLCN EQ 150050] N1

        (TAPS)
        IF[VTLCN EQ 200020] N1
        IF[VTLCN EQ 210010] N1
        IF[VTLCN EQ 220010] N1
        IF[VTLCN EQ 220010] N1
        IF[VTLCN EQ 220020] N1
        IF[VTLCN EQ 220030] N1
        IF[VTLCN EQ 220040] N1



    (OFF CENTER TOOLS / MULTI INSERT CHECK)
        (FACEMILLS)
        IF[VTLCN EQ 850020] N850020 (3" FACEMILL 9 FLT - SECO SQUARE 6)

(-------------------------------STOP  EDITS------------------------------)
(ALARM AND CENTER SET TOOL)

    (ALARM - TOOL JUMP N# NOT FOUND)
    NALM (TOOL N# NOT IN "OTDAT.LIB" TOOL LIBRARY)
    VUACM[1]='VERIFY TOOL #'
    VDOUT[993]=9001

    (CENTER SET TOOL VARIABLES)
    N1
    PX=0 PY=0 PRS=0
    GOTO NEND
(-------------------------------START EDITS------------------------------)
(NON-CENTER TOOL SET VARIABLES)

    N850020 (OFFSET TOOL - MULTI-SET/SINGLE)
    PX=37.1 PY=0 PRS=20 PINS=9
    GOTO NEND

    NEXAMP (EXAMPLE OFFSET TOOL - SINGLE SET)
    PX=0 PY=0 PRS=80
    GOTO NEND

(-------------------------------STOP  EDITS-----------------------------)

NEND
    (SET LOCALS AS VC#'S -- TO PASS BACK TO "OMTM.LIB" PGM)
    VC[PSRT]     = PX
    VC[PSRT + 1] = PY
    VC[PSRT + 2] = PRS
    VC[PSRT + 3] = PINS
RTS
